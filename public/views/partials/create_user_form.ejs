<form action="/users">
  <div class="form-group">
    <label for="createUserFirstName">First Name</label>
    <input id="createUserFirstName" type="text" class="form-control" name="firstName" placeholder="Enter first name">
  </div>
  <div class="form-group">
    <label for="createUserLastName">Last Name</label>
    <input id="createUserLastName" type="text" class="form-control" name="lastName" placeholder="Enter last name">
  </div>
  <div class="form-group">
    <label for="createUserEmail">Email</label>
    <input id="createUserEmail" type="text" class="form-control" name="loginId" placeholder="Enter email">
  </div>
  <div class="form-group">
    <label for="createUserPassword">Password</label>
    <input id="createUserPassword" type="password" class="form-control" name="password" placeholder="Password">  
  </div>
  <div class="form-group">
    <label for="createUserCourses">Number of Courses</label>
    <input id="createUserCourses" type="number" class="form-control" name="courses" value="3" placeholder="Number of Courses">  
  </div>
  <div class="form-group">
    <label for="createUserCourses">Enrollment Type</label>
    <select id="createUserCourses" class="custom-select" name="enrollmentType" placeholder="Enrollment Type">
      <option selected value="TeacherEnrollment">Teacher</option>
      <option value="StudentEnrollment">Student</option>
      <option value="TaEnrollment">Ta</option>
      <option value="ObserverEnrollment">Observer</option>
      <option value="DesignerEnrollment">Designer</option>
    </select>
  </div>
  <div class="form-group text-center">
    <input type="submit" class="btn btn-primary" style="width: 100%" value="Submit">  
  </div>
</form>
